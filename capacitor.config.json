{
  "appId": "app.coinsenda",
  "appName": "Coinsenda",
  "webDir": "build",
  "ios": {
    "webview": {
      "configuration": {
        "inspectable": true
      }
    },
    "contentInset": "always"
  },
  "server": {
    "androidScheme": "sendapp",
    "iosScheme": "sendapp",
    "url":  "http://192.168.1.6:2998",
    "cleartext":  true
  },
  "cordova": { 
    "preferences": {
      "overrideUserAgent": "Mozilla/5.0 (Linux; Android 6.0; Nexus 5 Build/MRA58N) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/89.0.4389.82 Mobile Safari/537.36"
    }
  },  
  "plugins": {
    "LocalNotifications": {
      "smallIcon": "ic_launcher",
      "iconColor": "#bde4ff"
    }
  }
}
