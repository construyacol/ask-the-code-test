  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start":"PORT=2998 cross-env react-scripts start",
    "start:dev":"cross-env NODE_ENV='development' npm start",
    "start:local":"cross-env REACT_APP_LOCAL_CONFIG=local npm run start:dev",
    "start:build":"npm run build && serve ./build -s -l 5000",
    "build-sw":"node ./src/sw.js",
    "deploy":"node deploy.js",
    "build": "react-scripts build && npm run build-sw && react-snapshot",
    "build:staging": "cross-env REACT_APP_BUILD_CONFIG='staging' npm run build",
    "build:pre_prod": "cross-env REACT_APP_BUILD_CONFIG='pre_prod' npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postbuild": "node ./scripts/preloads.js && ./rename_build_statics.sh"
  },
  "reactSnapshot": {
    "include": [
      "/wallets",
      "/withdraw_accounts",
      "/referral",
      "/security"
    ],
    "snapshotDelay": 300
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@node-minify/clean-css": "^6.1.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "node-minify": "^3.6.0",
    "prettier": "2.1.2",
    "react-snapshot": "^1.3.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "serve": "^11.2.0",
    "workbox-build": "^5.1.3"
  }
}
